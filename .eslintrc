{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "semi": ["error", "never"],
    "arrow-parens": [1, "as-needed"],
    "comma-dangle": ["error", "always-multiline"],
    "no-nested-ternary": [0, "never"],
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
